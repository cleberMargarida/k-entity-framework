{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/K.EntityFrameworkCore/**/*.csproj"],
          "exclude": ["**/bin/**", "**/obj/**"]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["docs/**.md", "*.md", "toc.yml"],
        "exclude": ["docs-backup/**"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "api/**", "_site/**"]
      }
    ],
    "dest": "bin/docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "K.EntityFrameworkCore Documentation",
      "_appName": "K.EntityFrameworkCore",
      "_gitContribute": {
        "repo": "https://github.com/cleberMargarida/k-entity-framework",
        "branch": "master"
      }
    }
  }
}
